#include <bits/stdc++.h>
using namespace std;

struct Node{
    int data;
    Node* next;
    Node(int d){data=d; next=NULL;}
};

int main(){
    Node* a = new Node(1);
    Node* b = new Node(2);
    Node* c = new Node(3);
    Node* d = new Node(4);
    a->next=b; b->next=c; c->next=d; d->next=b;

    unordered_set<Node*> s;
    Node* curr = a;
    while(curr){
        if(s.count(curr)){
            cout << "true";
            return 0;
        }
        s.insert(curr);
        curr = curr->next;
    }
    cout << "false";
}
